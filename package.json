{
  "name": "port-arranger",
  "version": "0.0.1",
  "description": "포트 충돌 없이 여러 개발 서버를 동시에 실행할 수 있게 해주는 도구",
  "type": "module",
  "bin": {
    "pa": "./dist/cli/index.js"
  },
  "main": ".vite/build/index.cjs",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "build:gui": "electron-forge package",
    "dev:gui": "electron-forge start",
    "make:gui": "electron-forge make"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "detect-port": "^1.6.1",
    "js-yaml": "^4.1.1",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-dmg": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-vite": "^7.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "electron": "^33.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.0",
    "vite": "^5.4.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
